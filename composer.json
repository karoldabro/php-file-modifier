{
    "name": "kdabrow/php-file-modifier",
    "description": "A library for making php files",
    "type": "package",
    "keywords": [
        "php file"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Karol DÄ…browski",
            "email": "karol_dabrowski@interia.eu",
            "homepage": "http://kdabrow.com"
        }
    ],
    "require": {
        "php": ">= 7.3",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3",
        "symfony/var-dumper": "^5.0",
        "phpmetrics/phpmetrics": "^2.5",
        "bruli/php-git-hooks": "^5.9"
    },
    "autoload": {
        "psr-4": {
            "Kdabrow\\PhpFileModifier\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kdabrow\\PhpFileModifier\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-install-cmd": [
          "PhpGitHooks\\Infrastructure\\Composer\\ConfiguratorScript::buildConfig"
        ],
        "post-update-cmd": [
          "PhpGitHooks\\Infrastructure\\Composer\\ConfiguratorScript::buildConfig"
        ]
    }
}
